{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": ["typescript", "import", "unicorn", "oxc"],
  "env": {
    "builtin": true,
    "browser": true
  },
  "categories": {
    "correctness": "error"
  },
  "rules": {
    "typescript/no-this-alias": "off",
    "unicorn/numeric-separators-style": "error"
  },
  "ignorePatterns": ["node_modules", ".temp", ".turbo"]
}
